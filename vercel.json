{
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python"
  },
  {
    "src": "build.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles"
    }
  }
],
  "routes": [
      {
        "src": "/(.*)",
        "dest": "api/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
  ]}